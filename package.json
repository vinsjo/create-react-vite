{
    "name": "@vinsjo/create-vite-react",
    "version": "1.1.6",
    "author": "Vincent Sj√∂gren",
    "license": "MIT",
    "description": "A package for creating a minimal react app using my preferred template setup, based on create-vite: https://github.com/vitejs/vite/tree/main/packages/create-vite",
    "repository": {
        "type": "git",
        "url": "https://github.com/vinsjo/create-vite-react"
    },
    "type": "module",
    "main": "index.js",
    "scripts": {
        "build": "npx unbuild -y",
        "prepublishOnly": "npm run build",
        "lint": "npx eslint src --ext .ts,.js",
        "format": "npx prettier -w src",
        "prepare": "husky install"
    },
    "files": [
        "index.js",
        "template-js",
        "template-ts",
        "dist"
    ],
    "bin": "index.js",
    "keywords": [
        "react",
        "vite",
        "frontend"
    ],
    "devDependencies": {
        "@types/minimist": "^1.2.2",
        "@types/prompts": "^2.0.14",
        "@typescript-eslint/eslint-plugin": "^5.36.2",
        "eslint": "^8.23.0",
        "kolorist": "^1.5.1",
        "minimist": "^1.2.6",
        "prettier": "^2.7.1",
        "pretty-quick": "^3.1.3",
        "prompts": "^2.4.2",
        "unbuild": "^0.8.9",
        "husky": "^8.0.0"
    }
}
